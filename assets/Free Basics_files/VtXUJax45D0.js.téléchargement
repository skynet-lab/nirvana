(function(){if(window===top||parent!==top)return;var a=null;document.addEventListener("DOMContentLoaded",b);window.addEventListener("load",b);window.addEventListener("unload",function(){window.parent.postMessage({unloading:!0},"*")});window.parent.postMessage({loading:!0},"*");function b(){if(document.documentElement==null)return;var b=document.documentElement.scrollHeight;if(a===b)return;window.parent.postMessage({height:b},"*");a=b}})();